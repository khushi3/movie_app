<%= form_for :movie, url: movies_path do |f| %>
    <p>
      <%= f.label :title %><br>
      <%= f.text_field :name %>
    </p>

    <p>
      <%= f.label :realeseDate %><br>
      <%= f.text_field :realese %>
    </p>
<p>
  <%= f.label :Actor %><br>
  <%=f.text_field :actor %>
</p>
    <p>
      <%= f.label :Director %><br>
      <%=f.text_field :director %>
    </p>
    <p>
      <%= f.submit %>
    </p>
<% end %>